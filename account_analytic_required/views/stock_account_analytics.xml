<odoo>
    <data>
        <record id="stock_account_analytics_field_form" model="ir.ui.view">
            <field name="name">stock.account.analytics.field.form</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='origin']" position="after">
                    <field name="stock_account_analytics" readonly="state in ['cancel', 'done']"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>